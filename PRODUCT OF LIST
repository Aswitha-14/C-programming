#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
  int n,i,pro=1;
  scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        pro=pro*a[i];
        
    }
    printf("The Product of list value is %.2f",(float)pro);
    return 0;
}
