#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int res,n,m;
    char o;
    scanf("%d",&n);
    scanf("%d",&m);
    scanf(" %c",&o);
    switch (o)
    {
    case '+':
    printf("Addition of two number is %.1f",(float)n+m);
    break;
    case '-':
    printf("Subtraction of two number is %.1f",(float)n-m);
    break;
    case '*':
    printf("Multiplication of two number is %.1f",(float)n*m);
    break;
    case '/':
    printf("Division of two number is %.1f",(float)n/m);
    break;
    case '%':
            res=n%m;
    printf("Modulo of two number is %.1f",(float)res);
            break;
    default:
    printf("Invalid Input");
    break;
}
return 0;}
