#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a;
    float b;
    char g,d[50],e[50];
    scanf("%d",&a);
    scanf("\n%f",&b);
    scanf("\n%c",&g);
    scanf("\n%s",d);
    scanf("\n%[^\n]s",e);
    printf("Integer is %d",a);
    printf("\nFloat is %.2f",b);
    printf("\nCharacter is %c",g);
    printf("\nStrings are %s and %s",d,e);
    return 0;
}
