#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    float a,b,c,d;
    scanf("%f\n%f",&a,&b);
    c=b/100;
    d=(a)/(c*c);
    if(d<18.5)
    
       {
        printf("You are underweight. Have an apple daily.");
}
        else if(18.5<d && d<24.9){
            printf("You are normal. Go walking daily and maintain it.");
        }
        else if(25<d && d<29.9){
            printf("You are overweight. Go to Gym daily.");
        }
    else
    {
        printf("You are obese. You need to see a doctor.");
    }
    


    return 0;
}
