#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int n;
    int i,current=6;
    int diff=3;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        printf("%d ",current);
        current=current+diff;
        diff=diff+2;
        
    }
    return 0;
}
