#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char name[50];
    int a,b;
    scanf("%s",name);
    scanf("%d\n%d",&a,&b);
    if(a==1 && b>70)
    {
        printf("Name of the Student:%s",name);
        printf("\n%s is Eligible for Placement",name);
        
}
    else if((a==1|| a==2) && (b>75))
    {
        printf("Name of the Student:%s",name);
        printf("\n%s is Eligible for Placement",name);
}
    else
    {
        printf("Name of the Student:%s",name);
        printf("\n%s is Not Eligible for Placement",name);
}
    return 0;
}
