#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#define pi 3.1415926359

int main() {

    int a,b,c,d,e,f,g;
    scanf("%d",&a);
    scanf("%d",&b);
    scanf("%d",&c);
    scanf("%d",&d);
    scanf("%d",&e);
    scanf("%d",&f);
    scanf("%d",&g);
    printf("Perimeter of Square:%d",4*a);
    printf("\nPerimeter of Rectangle:%d",2*(b+c));
    float x=pi*2*d;
    float final=(floor)(x*100)/100;
    printf("\nPerimeter of Circle:%.2f",final);
    printf("\nPerimeter of Triangle:%d",e+f+g);
    return 0;
}
